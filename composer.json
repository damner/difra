{
    "name": "difra/difra",
    "description": "Difra framework",
    "keywords": [
        "framework",
        "php"
    ],
    "homepage": "https://www.difra.org",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=7.0.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-session": "*",
        "ext-SimpleXML": "*",
        "ext-xsl": "*",
        "ext-zlib": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.a-jam.net/scm/difra/difra.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Difra\\": "src"
        }
    }
}
