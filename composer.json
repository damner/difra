{
    "name": "difra/difra",
    "description": "Difra framework",
    "keywords": [
        "framework",
        "php"
    ],
    "homepage": "https://www.difra.org",
    "license": "BSD-2-Clause",
    "require": {
        "php": "~7.0.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-session": "*",
        "ext-SimpleXML": "*",
        "ext-xsl": "*",
        "ext-zlib": "*",
        "oyejorge/less.php": "~1.7.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "classmap": [
            "src/main.php"
        ],
        "psr-4": {
            "Difra\\": "src"
        }
    }
}
