<?xml version="1.0" encoding="utf-8"?>
<phpdox xmlns="http://phpdox.de/config">
	<project name="difra" source="${basedir}/.." workdir="${basedir}/phpdox">
		<collector publiconly="false">
			<include mask="*.php"/>
			<exclude mask="**/fw/lib/libs/esapi/**"/>
			<exclude mask="**/fw/lib/libs/less/**"/>
		</collector>

		<generator output="${basedir}">
			<build engine="html" enabled="true" output="api"/>
		</generator>
	</project>
</phpdox>